[access]
	inheritFrom = Public-Projects
[project]
	description = repo - The Multiple Git Repository Tool
[receive]
	requireChangeId = true
	maxObjectSizeLimit = 10M
[submit]
	action = rebase always
	rejectEmptyCommit = true

[label "Code-Review"]
	function = NoBlock
	copyMinScore = true
	copyMaxScore = true
	copyAllScoresOnTrivialRebase = true
	copyAllScoresIfNoCodeChange = true
	abbreviation = CR
	value = -2 Do not submit
	value = -1 I would prefer that you didn't submit this
	value = 0 No score
	value = +1 Looks good to me, but someone else must approve
	value = +2 Looks good to me, approved
	defaultValue = 0
[label "Verified"]
	function = NoBlock
	copyMinScore = true
	copyMaxScore = true
	copyAllScoresOnTrivialRebase = true
	copyAllScoresIfNoCodeChange = true
	abbreviation = V
	value = -1 Fails
	value = 0 No score
	value = +1 Verified
	defaultValue = 0

[access "refs/*"]
	owner = group repo-maintainer
	forgeAuthor = group repo-contributor
	exclusiveGroupPermissions = pushMerge
[access "refs/for/refs/heads/stable"]
	exclusiveGroupPermissions = push
[access "refs/heads/*"]
	label-Verified = -1..+1 group Change Owner
	label-Verified = -1..+1 group repo-contributor
	label-Verified = -1..+1 group repo-maintainer
	label-Code-Review = -2..+2 group repo-contributor
	label-Code-Review = -2..+2 group repo-maintainer
	submit = group Change Owner
	submit = group repo-maintainer
	rebase = group repo-contributor
	abandon = group repo-contributor
[access "refs/heads/maint"]
	push = +force group repo-maintainer
[access "refs/heads/stable"]
	push = +force group repo-maintainer
[access "refs/tags/*"]
	createTag = group repo-maintainer
	createSignedTag = group repo-maintainer
[access "refs/for/refs/meta/config"]
	push = group repo-maintainer
[access "refs/meta/config"]
	read = group repo-maintainer
	label-Code-Review = -2..+2 group repo-maintainer
	label-Verified = -1..+1 group repo-maintainer
	submit = group repo-maintainer
[access "refs/for/*"]
	addPatchSet = group repo-contributor
	addPatchSet = group repo-maintainer
	exclusiveGroupPermissions = addPatchSet
[access "refs/for/refs/heads/master"]
	exclusiveGroupPermissions = push
[submit-requirement "Code-Review"]
	description = Submit requirement for the 'Code-Review' label
	submittableIf = label:Code-Review=MAX AND -label:Code-Review=MIN
	canOverrideInChildProjects = true
[submit-requirement "Verified"]
	description = Submit requirement for the 'Verified' label
	submittableIf = label:Verified=MAX AND -label:Verified=MIN
	canOverrideInChildProjects = true
[commentlink "ph"]
	match = "([Bb]ug|[Ii]ssue)\\s*#?(\\d+)"
	link = https://code.google.com/p/git-repo/issues/detail?id=$2
