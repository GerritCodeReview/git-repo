[project]
	description = repo - The Multiple Git Repository Tool
[receive]
	requireChangeId = true
[submit]
[access "refs/*"]
	owner = group repo-maintainer
[access "refs/for/refs/heads/stable"]
	exclusiveGroupPermissions = push
[access "refs/heads/*"]
	label-Verified = -1..+1 group Change Owner
	label-Verified = -1..+1 group repo-contributor
	label-Verified = -1..+1 group repo-maintainer
	label-Code-Review = -2..+2 group repo-maintainer
	submit = group repo-maintainer
	rebase = group repo-contributor
	abandon = group repo-contributor
[access "refs/heads/maint"]
	push = group repo-maintainer
[access "refs/heads/stable"]
	exclusiveGroupPermissions = createReview push
	push = group repo-maintainer
[access "refs/tags/*"]
	createTag = group repo-maintainer
	createSignedTag = group repo-maintainer
[access]
	inheritFrom = Public-Projects
[access "refs/for/refs/meta/config"]
	push = group repo-maintainer
	pushMerge = group repo-maintainer
[access "refs/meta/config"]
	read = group repo-maintainer
	label-Code-Review = -2..+2 group repo-maintainer
	label-Verified = -1..+1 group repo-maintainer
	submit = group Change Owner
	submit = group repo-maintainer
	createReview = group repo-maintainer
	pushMerge = group repo-maintainer
[commentlink "ph"]
	match = "([Bb]ug|[Ii]ssue)\\s*#?(\\d+)"
	link = https://code.google.com/p/git-repo/issues/detail?id=$2
